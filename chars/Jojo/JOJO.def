;===========================================================================

;Stick Figure Information
[Info]
name = "JoJo"        ;Name of character
displayname = "JO-JO" ;Name of character to display
versiondate = 4,18,2020    ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.0        ;Version of M.U.G.E.N character works on (X.XX)
author = "Programmed by Taffy, Z & Bonehouse"         ;Character author nam
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 280,240        ;Local coordinate space width and height
ikemenversion=0.99

; Files for the player
[Files]
cmd     = Jojo.cmd                    ;Command set
cns     = Jojo.cns                        ;Constants
st      = Jojo.cns                          ;States
st1      = Normals.cns                ;Normals
st2      = Specials.cns                ;Specials
st3      = HyperCombos.cns     ;Supers
st4      = Target.cns                   ;Target
st5      = Helpers.cns                 ;Helpers
st6      = movement.cns                 
stcommon = common1.cns     ;Common states (from data/ or motif)
sprite  = Jojo.sff                        ;Sprite
anim    = Jojo.air                       ;Animation
sound   = Jojo.snd                    ;Sound
ai      = Jojo.ai                            ;AI hints data (not used)
movelist=Jojo.dat

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6
x2 = 7 ;Hold Start and press button X for palette 7, etc.
y2 = 8
z2 = 9
a2 = 10
b2 = 11
c2 = 12

